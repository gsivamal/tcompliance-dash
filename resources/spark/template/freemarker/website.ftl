		<script type="text/javascript">
		
			$("#slideshow > div:gt(0)").hide();
			
			setInterval(function() { 
			  $('#slideshow > div:first')
			    .fadeOut(1500)
			    .next()
			    .fadeIn(1500)
			    .end()
			    .appendTo('#slideshow');
			},  3000);
			
			
			 $('#scrolldown').click(function() {
		        $('html, body').animate({
		            scrollTop: $('#appstore').offset().top
		         }, 'slow');
		     });

		</script>
			
		<script type="text/javascript">
		
			var currentIndex = 0,
			  items = $('.slidecontainer div'),
			  itemAmt = items.length;
			
			function cycleItems() {
			  var item = $('.slidecontainer div').eq(currentIndex);
			  items.hide();
			  item.css('display','inline-block');
			}
			
			var autoSlide = setInterval(function() {
			  currentIndex += 1;
			  if (currentIndex > itemAmt - 1) {
			    currentIndex = 0;
			  }
			  //cycleItems(); //salai: to stop auto slide
			}, 3000);
			
			$('#nextslide').click(function() {
			  clearInterval(autoSlide);
			  currentIndex += 1;
			  if (currentIndex > itemAmt - 1) {
			    currentIndex = 0;
			  }
			  cycleItems();
			});
			
			$('#prevslide').click(function() {
